{
    "name": "odutola/laravel-payant",
    "description": "A Laravel 6 Package for Payant",
    "keywords": ["php","github", "laravel","Open Source","payments", "subscription", "payant", "payant.co","laravel 5"],
    "license": "MIT",
    "authors": [
      {
        "name": "odutolaabisoye",
        "email": "odutolaabisoye@gmail.com"
      },
      {
        "name": "odutolaabisoye",
        "email": "odutolaabisoye@gmail.com"
      }
   ],
   "minimum-stability": "stable",
   "require": {
    "php": "^5.4.0|^7.0",
    "illuminate/support": "5.*",
    "guzzlehttp/guzzle": "5.*|6.*"
   },
   "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "satooshi/php-coveralls": "^0.7.0",
        "mockery/mockery": ">=0.7.2"
    },
   "autoload": {
        "psr-4": {
          "Odutola\\Payant\\": "src/"
        }
   },
   "autoload-dev": {
        "psr-4": {
            "Odutola\\Payant\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
   "extra": {
    "laravel": {
        "providers": [
            "Odutola\\Payant\\PayantServiceProvider"
        ],
        "aliases": {
            "Payant": "Odutola\\Payant\\Facades\\Payant"
        }
    }
  }
}
